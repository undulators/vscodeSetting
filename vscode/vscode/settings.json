{
  //vscode base setting
  //"terminal.integrated.shell.windows": "D:\\git\\Git\\bin\\bash.exe",
  "files.autoSave": "afterDelay",
  "breadcrumbs.enabled": true,
  "editor.fontSize": 18,
  "editor.tabSize": 2,
  "window.zoomLevel": 0,
  //vscode projectmanager:Project Manager 
  "projectManager.vscode.baseFolders": [
    "D:\\vscode\\workspace"
  ],
  //vscode Templates :Blueprint
  "blueprint.templatesPath": [
    "./blueprint-templates"
  ],
  //vscode keymap:Eclipse Keymap 
  "alphabotsec.vscode-eclipse-keybindings":[],
  //vscode gitbash:Start git-bash
  "mccarter.start-git-bash":[],
  //vscode theme:Material Icon Theme、Material Theme 
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#80CBC4",
    "activityBar.activeBorder": "#80CBC4",
    "list.activeSelectionForeground": "#80CBC4",
    "list.inactiveSelectionForeground": "#80CBC4",
    "list.highlightForeground": "#80CBC4",
    "scrollbarSlider.activeBackground": "#80CBC450",
    "editorSuggestWidget.highlightForeground": "#80CBC4",
    "textLink.foreground": "#80CBC4",
    "progressBar.background": "#80CBC4",
    "pickerGroup.foreground": "#80CBC4",
    "tab.activeBorder": "#80CBC4",
    "notificationLink.foreground": "#80CBC4",
    "editorWidget.resizeBorder": "#80CBC4",
    "editorWidget.border": "#80CBC4",
    "settings.modifiedItemIndicator": "#80CBC4",
    "settings.headerForeground": "#80CBC4",
    "panelTitle.activeBorder": "#80CBC4",
    "breadcrumb.activeSelectionForeground": "#80CBC4",
    "menu.selectionForeground": "#80CBC4",
    "menubar.selectionForeground": "#80CBC4",
    "editor.findMatchBorder": "#80CBC4",
    "selection.background": "#80CBC440"
  },
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.color": "#26a69a",
  "materialTheme.accent": "Teal",
  //vscode Highlighter: Rainbow Highlighter、indent-rainbow、Bracket Pair Colorizer 2
  "rainbow-highlighter.background-alpha": 0.27,
  "rainbow-highlighter.palette": [
    "rgb(255,255,64)",
    "rgb(127,255,127)",
    "rgb(255,127,255)",
    "rgb(79,236,236)"
  ],
  "rainbow-highlighter.use-border": true,
  "indentRainbow.colors": [
    "rgba(255,255,64,0.07)",
    "rgba(127,255,127,0.07)",
    "rgba(255,127,255,0.07)",
    "rgba(79,236,236,0.07)"
  ],
  "indentRainbow.errorColor": "rgba(128,32,32,0.6)",
  "indentRainbow.excludedLanguages": [
    "plaintext"
  ],
  "indentRainbow.ignoreErrorLanguages": [
    "markdown"
  ],
  "indentRainbow.ignoreLinePatterns": [
    "/[ \t]* [*]/g",
    "/[ \t]+[/]{2}/g"
  ],
  "indentRainbow.includedLanguages": [],
  "indentRainbow.indentSetter": {},
  "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
  "indentRainbow.updateDelay": 100,
  "bracket-pair-colorizer-2.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}",
    "opacity: 0.5"
  ],
  "bracket-pair-colorizer-2.colorMode": "Consecutive",
  "bracket-pair-colorizer-2.colors": [
    "Gold",
    "Orchid",
    "LightSkyBlue"
  ],
  "bracket-pair-colorizer-2.excludedLanguages": [],
  "bracket-pair-colorizer-2.rulerPosition": "Center",
  "bracket-pair-colorizer-2.scopeLineRelativePosition": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": true,
  "bracket-pair-colorizer-2.showVerticalScopeLine": true,
  "bracket-pair-colorizer-2.unmatchedScopeColor": "Red",
  //vscode translate:Comment Translate
  "commentTranslate.targetLanguage": "zh-CN",
  "bookmarks.navigateThroughAllFiles": true,
  "bookmarks.showCommandsInContextMenu": true,
  "bookmarks.wrapNavigation": true,
  //vscode spell check；Code Spell Checker
  "cSpell.enabled": true,
  //vscode git:git-autoconfig、GitLens
  "git-autoconfig.configList": [
    {
      "user.email": "504898441@qq.com",
      "user.name": "undulator"
    }
  ],
  "gitlens.views.compare.enabled": false,
  "gitlens.views.search.enabled": false,
  "files.associations": {
    "stdio.h": "c"
  },
  //VIM setting
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.insertModeKeyBindings": [
    // {
    //   "before": ["j", "j"],
    //   "after": ["<Esc>"]
    // }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"] 
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"], 
      "after": ["$"]
    }
    {
      "before": ["T"],
      "after": ["g","t"]
    },
    {
      "before": ["R"],
      "after": ["g","T"]
    }
    {
      "before": ["<C-h>"],
      "after": ["<C-e>"]  
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-y>"]
    },
    {
      "before": ["<C-m>"],
      "after": ["`","m"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-y>": false,
    "<C-e>": false,
    "<C-v>": false,
    "<C-c>": false
  }
}